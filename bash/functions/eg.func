#!/usr/bin/env bash

# `eg` is a shorthand for `egrep` with the option to pass in multiple
# words to search for
#
# usage:
#
#   `cat src/resources/config/SourceCandidatePipelineDaemon-common-config.properties | egf port host throttle`
function eg() {
  local y;
  y="$@";
  local words="($(echo -n $y | sed 's/ /|/g'))"
  while read data; do
    echo -n $data | egrep -i $words;
  done
}
