#!/bin/bash

# Transpose first log entry and sort
function transpose_first_and_sort() {
  if [ $# -ne 1 ]; then
    echo "Error: No file specified."
    return 1
  fi

  local filename=$1
  if [ -f $filename ]; then
    head -n1 $filename | transpose-log-entries | sort
  else
    echo "'$filename' is not a valid file"
  fi
}

